<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">
    <filter>
        <filter-name>Filter</filter-name>
        <filter-class>neuedu.tools.filter.CharacterFilter</filter-class>
    </filter>
    <filter-mapping>
        <filter-name>Filter</filter-name>
        <url-pattern>*</url-pattern>
    </filter-mapping>




    <servlet>
        <servlet-name>test</servlet-name>
        <servlet-class>test</servlet-class>
        <load-on-startup>2</load-on-startup>
    </servlet>
    <servlet-mapping>
        <servlet-name>test</servlet-name>
        <url-pattern>/a</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>newtest</servlet-name>
        <servlet-class>newtest</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>newtest</servlet-name>
        <url-pattern>/b</url-pattern>
    </servlet-mapping>





    <servlet>
        <servlet-name>Login</servlet-name>
        <servlet-class>neuedu.test.day1212.controller.Login</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>Resign</servlet-name>
        <servlet-class>neuedu.test.day1212.controller.Resign</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Resign</servlet-name>
        <url-pattern>/resign</url-pattern>
    </servlet-mapping>



    <servlet>
        <servlet-name>List</servlet-name>
        <servlet-class>neuedu.test.day1213.controller.ListsServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>List</servlet-name>
        <url-pattern>/list</url-pattern>
    </servlet-mapping>



</web-app>